{
  "name": "loopback",
  "script": "server/server.js",
  "cwd": "./",
  "max_restarts": 5,
  "restart_delay": 4000,
  "instances": 1,
  "exec_mode": "fork",
  "watch": [
    "server"
  ],
  "max_memory_restart": "1G",
  "out_file": "loopback-logs/log.log",
  "error_file": "loopback-logs/error.log",
  "log_date_format": "YYYY-MM-DD HH:mm Z"
}